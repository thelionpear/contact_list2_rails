<h1>Edit Contact</h1>
<%= form_for @contact do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :age %>
    <%= f.select(:age, (18..100).to_a) %>

    <%= f.label :hair_color %>
    <%= f.text_field :hair_color %>

    <%= f.label :eye_color %>
    <%= f.text_field :eye_color %>

    <%= f.label :gender %>
    <%= f.text_field :gender %>

    <%= f.label :alive %>
    <%= f.select(:alive, [["true", true],["false", false]])  %> 

    <%= f.submit %>
<% end %> 